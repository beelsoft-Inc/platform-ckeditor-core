{
    "name": "tungnt/mrkun-ckeditor",
    "description": "Easy way to use ckeditor in laravel-admin",
    "license": "MIT",
    "authors": [
        {
            "name": "tungnt",
            "email": "tungnt.code@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "laravel/framework": ">=9.0",
        "pimple/pimple": "~3.0",
        "league/flysystem": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-azure-blob-storage": "^3.0",
        "league/flysystem-ftp": "^3.0",
        "spatie/flysystem-dropbox": "^2.0",
        "ext-json": "*",
        "ext-gd": "*",
        "ext-zip":  "*"
    },
    "autoload": {
        "psr-4": {
            "Tungnt\\CKEditor\\": "src",
            "CKSource\\CKFinder\\": "_connector/"
        }
